<VirtualHost *:443>
        ServerAdmin admin@example.org
        ServerName example.org
        ServerAlias www.example.org
        DocumentRoot /var/www/html/example.org/public_html//public_html
        ErrorLog /var/www/html/example.org/public_html//logs/error.log
        <Directory /var/www/html/example.org/public_html//public_html>
                Require all granted
        </Directory>
</VirtualHost>
