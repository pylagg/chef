<VirtualHost *:80>
        ServerAdmin admin@example.com
        ServerName example.com
        ServerAlias www.example.com
        DocumentRoot /var/www/html/example.com/public_html//public_html
        ErrorLog /var/www/html/example.com/public_html//logs/error.log
        <Directory /var/www/html/example.com/public_html//public_html>
                Require all granted
        </Directory>
</VirtualHost>
